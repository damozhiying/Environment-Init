// 将键绑定放入此文件中以覆盖默认值
[
{ "key": "ctrl+d",                "command": "editor.action.deleteLines",
                                     "when": "editorTextFocus && !editorReadonly" },
{ "key": "ctrl+e",                "command": "workbench.action.reopenClosedEditor" },
{ "key": "ctrl+l",                "command": "workbench.action.terminal.clear",
                                     "when": "terminalFocus" },
{ "key": "alt+/",                 "command": "editor.action.quickFix",
                                     "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly" },
{ "key": "ctrl+shift+/",          "command": "editor.action.blockComment",
                                     "when": "editorTextFocus && !editorReadonly" },
{ "key": "shift+alt+z",           "command": "workbench.action.toggleZenMode" },
{ "key": "f3",                    "command": "editor.action.nextMatchFindAction",
                                     "when": "editorHasSelection" },
{ "key": "f4",                    "command": "editor.action.previousMatchFindAction",
                                     "when": "editorHasSelection" },
{ "key": "f3",                    "command": "editor.action.nextSelectionMatchFindAction",
                                     "when": "editorHasSelection" },
{ "key": "f4",                    "command": "editor.action.previousSelectionMatchFindAction",
                                     "when": "editorHasSelection" },
{ "key": "f3",                    "command": "search.action.focusNextSearchResult" ,
                                     "when": "searchViewletVisible" },
{ "key": "f4",                    "command": "search.action.focusPreviousSearchResult" ,
                                     "when": "searchViewletVisible" },
{ "key": "f3",                    "command": "workbench.action.previousEditor" ,
                                     "when": "editorFocus && !editorHasSelection" },
{ "key": "f4",                    "command": "workbench.action.nextEditor" ,
                                     "when": "editorFocus && !editorHasSelection" }
// 最后一个没有末尾逗号
]